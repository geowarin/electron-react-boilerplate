{
  "name": "electron-react-boilerplate",
  "productName": "ElectronReact",
  "version": "0.7.1",
  "description": "Electron application boilerplate based on React, React Router, Webpack, React Hot Loader for rapid application development",
  "main": "main.js",
  "scripts": {
    "hot:server": "node server.js",
    "hot:client": "cross-env NODE_ENV=development electron ./",
    "start": "npm-run-all --parallel hot:*",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "build:package": "node package.js",
    "package": "npm-run-all build:*",
    "package:all": "npm-run-all 'build:* -- --all'",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chentsulin/electron-react-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chentsulin/electron-react-boilerplate/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/chentsulin/electron-react-boilerplate#readme",
  "devDependencies": {
    "asar": "^0.9.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "electron-debug": "^0.5.1",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.7",
    "enzyme": "^1.4.1",
    "expect": "^1.13.4",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^1.2.0",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.1",
    "react-addons-test-utils": "^0.14.7",
    "redux-devtools": "^3.0.2",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "webpack": "^1.12.1",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "history": "^1.17.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-photonkit": "^0.4.1",
    "react-redux": "^4.1.2",
    "react-router": "^1.0.3",
    "redux": "^3.1.6",
    "react-router-redux": "^2.1.0"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
