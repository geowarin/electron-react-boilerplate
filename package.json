{
  "name": "electron-react-boilerplate",
  "productName": "ElectronReact",
  "version": "0.7.1",
  "description": "Electron application boilerplate based on React, React Router, Webpack, React Hot Loader for rapid application development",
  "main": "main.js",
  "scripts": {
    "hot-server": "webpack-dev-server --config=webpack.config.development.js --hot --inline --colors",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "start-hot": "better-npm-run start-hot",
    "package": "better-npm-run package",
    "package-all": "npm run package -- --all"
  },
  "betterScripts": {
    "start": {
      "command": "electron ./",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-hot": {
      "command": "electron ./",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "package": {
      "command": "node package.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chentsulin/electron-react-boilerplate.git"
  },
  "author": {
    "name": "C. T. Lin",
    "email": "chentsulin@gmail.com",
    "url": "https://github.com/chentsulin"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chentsulin/electron-react-boilerplate/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/chentsulin/electron-react-boilerplate#readme",
  "devDependencies": {
    "asar": "^0.9.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-2": "^6.3.13",
    "better-npm-run": "0.0.5",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "electron-debug": "^0.5.1",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "isdev": "^1.0.1",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "font-awesome": "^4.4.0",
    "history": "^1.17.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
